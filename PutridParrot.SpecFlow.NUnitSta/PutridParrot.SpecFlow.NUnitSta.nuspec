<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>PutridParrot.SpecFlow.NUnitSta</id>
    <version>1.0.1-alpha</version>
    <title>PutridParrot.SpecFlow.NUnitSta</title>
    <authors>Mark Timmings</authors>
    <owners>PutridParrot</owners>
    <description>This is a SpecFlow Plugin to add ApartmentState.STA to classes</description>
    <summary>For some forms of UI automation testing using SpecFlow, we need
    to mark NUnit as STA thread model. This package simply does this as part of the 
    SpecFlow code generations - it's all or nothing approach, i.e. we simple set this attribute
    on every class generated.</summary>
    <language>en-GB</language>
	<icon>PutridParrot.SpecFlow.NUnitSta.png</icon>
    
    
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>specflow</tags>
    <!--<dependencies>
      <dependency id="SpecFlow" version="insert specflow version here"/>
    </dependencies>-->
  </metadata>
  <files>
    <file src="build\**\*" target="build"/>

    <file src="..\PutridParrot.SpecFlow.NUnitSta.png" target="" />
    
    <file src="bin\$config$\net472\PutridParrot.SpecFlow.NUnitSta.*" target="build\net472"/>
    <file src="bin\$config$\netcoreapp3.1\PutridParrot.SpecFlow.NUnitSta.dll" target="build\netcoreapp3.1"/>
    <file src="bin\$config$\netcoreapp3.1\PutridParrot.SpecFlow.NUnitSta.pdb" target="build\netcoreapp3.1"/>


  </files>
</package>